{
  "name": "azure-search-openai-javascript",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "concurrently \"npm:start:*\" --kill-others",
    "start:webapp": "npm run dev --workspace=webapp",
    "start:api": "npm run dev --workspace=api",
    "test": "npm run test -ws --if-present",
    "build": "npm run build -ws --if-present",
    "build:webapp": "npm run build --workspace=webapp",
    "format": "prettier --list-different --write .",
    "format:check": "prettier --check .",
    "lint": "npm run lint -ws --if-present",
    "lint:fix": "npm run lint:fix -ws --if-present",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Microsoft",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "concurrently": "^8.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "bracketSpacing": true
  }
}
