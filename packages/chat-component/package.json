  "description": "A minimal OpenAI chat web component to hook as a client to any backend implementation",
  "main": "main.js",
  "module": "main.js",
  "type": "module",
  "scripts": {
    "clean": "cd dist && rimraf chat-component.{d.ts,d.ts.map,js,js.map} ",
